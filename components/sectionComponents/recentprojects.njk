<section class="work" id="work">
    <div class="work-container">
        {% for card in activeComponent.cardlist %}
            <div class="work-item">
                <img src="{{ card.image }}" alt="{{ alt }}"/>
                <h3>{{ card.title }}</h3>
                <p>{{ card.description | safe }}</p>
            </div>
        {% endfor %}
    </section>